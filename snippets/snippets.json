{
  "template notice": {
    "prefix": "b-notice",
    "body": [
      "\ninclude ./../../../../../../src/templates/_mixin\n  div.vh100.row-flex\n\tdiv(class=\"row ml40 m-a\")\n\t\t+noticeModalHeader\n\t\t+noticeModalSection"
    ],
    "description": "crea una pagina de notice"
  },
  "template Recorder": {
    "prefix": "b-recorder",
    "body": [
      "div(class=\"row mt30\"): +aeRecorder('a1m1a-aa', 1, 60)\n\n\t+libScripts([\"audios/audio-recorder-safari.js\"])\n"
    ],
    "description": "template de recorder"
  },
  "template Image": {
    "prefix": "b-img",
    "body": ["+image(${unit}, ${module}, namePage+'_img_1.png')\n"],
    "description": "template de imagen"
  },
  "template Video": {
    "prefix": "b-vid",
    "body": ["+video(${unit}, ${module}, namePage+'_vid_1').centrado\n"],
    "description": "template de video"
  },
  "template Audio": {
    "prefix": "b-aud",
    "body": ["+audio(${unit}, ${module}, namePage+'_aud_1').centrado\n"],
    "description": "template de audio"
  },
  "template selfcheck": {
    "prefix": "b-pug-self",
    "body": [
      "+tableSelfReview(\"LANGUAGE\", \n\t  [ \n\t\t\t{ \n\t\t\t\t title: '', \n\t\t\t\t options: [ \n\t\t\t\t\t  {value: ''},\n\t\t\t\t\t  {value: ''},\n\t\t\t\t\t  {value: ''},\n\t\t\t\t\t  {value: ''}\n\t\t\t\t ]\n\t\t\t}\n\t  ]\n )(table-width=\"sm\") \n"
    ],
    "description": "template de tabla de selfcheck"
  },
  "template gameClone": {
    "prefix": "b-pug-clone",
    "body": [
      "\n-var arrWords = []\n-var url = \"${unit}/${module}/\"+namePage\n-var contentId = \"${miContenido}\"\n-var modalId = \"${miModal}\"\n+gameClone({ arrWords, contentId })\n+gameCloneModal({arrWords , modalId, url}))\nscript.\n    fn_flickity_init();\n\t$(document).ready(function(){\n\t\t$('#${miModal}').modal();\n\t});\n\tfun_cloneImage( document.getElementById('${miModal}'), document.getElementById('${miContenido}'));\n"
    ],
    "description": "template de gameClone"
  },
  "template libScripts": {
    "prefix": "b-libScripts",
    "body": ["|\n+libScripts([\"${fileRoute}\"])\n"],
    "description": "mixin para agregar librerias js"
  },
  "template libStyles": {
    "prefix": "b-libStyles",
    "body": ["|\n+libStyles([\"${fileRoute}\"])\n"],
    "description": "mixin para agregar librerias css"
  },
  "template noticeModal": {
    "prefix": "b-noticeModal",
    "body": [
      "\n\t\tbutton(class=\"c-btn-IconAction openNotice is-active u-bgIconAlert\" ng-click=\"idNotice = 1\" )\n+noticeModal(${level}, ${module}, ${template})"
    ],
    "description": "template de mixin del notice"
  },
  "template random function": {
    "prefix": "b-random",
    "body": [
      "\n-var arrRandom = [1, 2, 3, 4, 5]\n- arrRandom.sort((a,b)=>{return Math.random()-0.5})\neach item, index in arrRandom\n\t"
    ],
    "description": "template de random"
  },
  "template create drags": {
    "prefix": "b-drags",
    "body": ["\n+eachDrag(${arrDrags})\n\t"],
    "description": "template de crear drags"
  },
  "template flickity": {
    "prefix": "b-flickity",
    "body": [
      "\n+flickityBase\n\t+flickityControls(${cantSlides})\n\t\tdiv(class=\"c-flickity__cell\"): .row\n\t",
      "|\nscript.\n\tfn_flickity_init();\n\t"
    ],
    "description": "template de flickity"
  },
  "template cardColors": {
    "prefix": "b-cardColors",
    "body": ["\n.c-cardColors\n\t.c-cardColorsContent\n\t\t${contentCard}\n"],
    "description": "template de cardColors"
  },
  "template table": {
    "prefix": "b-table",
    "body": [
      "\ntable.c-table(table-type=\"${tableStyle}\")\n\tthead\n\t\ttr\n\t\t\tth\n\ttbody\n\t\ttr\n\t\t\ttd\n"
    ],
    "description": "template de tablas"
  },
  "template Game Styles": {
    "prefix": "b-gameStyle",
    "body": ["\n+gameStyles([\"${urlStyle}\"])"],
    "description": "template de url de estilo de juegos"
  },
  "template Game Script": {
    "prefix": "b-gameScript",
    "body": ["\n+gameScripts([\"${urlScript}\"])"],
    "description": "template de url de estilo de juegos"
  }
}
